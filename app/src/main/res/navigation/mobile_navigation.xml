<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    app:labelVisibilityMode="unlabeled">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sleewell.sleewell.mvp.home.view.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_music"
        android:name="com.sleewell.sleewell.nav.music.MusicFragment"
        tools:layout="@layout/fragment_music" />

    <fragment
        android:id="@+id/navigation_alarms"
        android:name="com.sleewell.sleewell.nav.alarms.AlarmsFragment"
        tools:layout="@layout/fragment_alarms" />

    <fragment
        android:id="@+id/navigation_statistics"
        android:name="com.sleewell.sleewell.mvp.statistics.view.StatisticsFragment"
        tools:layout="@layout/fragment_statistics" />

</navigation>